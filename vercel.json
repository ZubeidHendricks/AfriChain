{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "main_tidb.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/hedera/ai-studio-agent.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/hedera/natural-language.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/hedera/status.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/hedera/test-connection.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/hedera/(.*)",
      "dest": "/api/hedera/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "main_tidb.py"
    },
    {
      "src": "/docs",
      "dest": "main_tidb.py"
    },
    {
      "src": "/redoc",
      "dest": "main_tidb.py"
    },
    {
      "src": "/health",
      "dest": "main_tidb.py"
    },
    {
      "src": "/dashboard",
      "dest": "main_tidb.py"
    },
    {
      "src": "/agents",
      "dest": "main_tidb.py"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "https://verichain-x-hedera.vercel.app",
    "NODE_ENV": "production",
    "PYTHONPATH": ".",
    "ENVIRONMENT": "production",
    "DEBUG": "false",
    "HACKATHON_MODE": "true",
    "HEDERA_ACCOUNT_ID": "0.0.6503585",
    "HEDERA_PRIVATE_KEY": "0x8b8add46cb9415d0a2429fef41cc6758232ec2a5977b4b234145d0b897ad03eb",
    "HEDERA_NETWORK": "testnet"
  }
}